# Caluma
# ------------------------------------------------------------------------------
ENV=dev
OIDC_VERIFY_SSL=False
OIDC_USERINFO_ENDPOINT=https://mysagw.local/auth/realms/mysagw/protocol/openid-connect/userinfo
# Multiple ADMINS need to be comma separated
ADMINS=Test Example <test@example.com>
DISABLE_INTROSPECTION=false

# Minio config
MINIO_PRESIGNED_TTL_MINUTES=30
MINIO_STORAGE_ACCESS_KEY=mysagw-minio
MINIO_STORAGE_ENDPOINT=mysagw.local
MINIO_STORAGE_MEDIA_BUCKET_NAME=caluma-media
MINIO_STORAGE_SECRET_KEY=mysagw-minio123
MINIO_STORAGE_USE_HTTPS=true
MINIO_DISABLE_CERT_CHECKS=true

# Mailing
EMAIL_HOST=mail
EMAIL_PORT=1025
MAILING_FROM_NAME=mySAGW
MAILING_FROM_MAIL=noreply@mysagw.adfinis.com
MAILING_SENDER="mySAGW <noreply@mysagw.adfinis.com>"

# URIs
API_BASE_URI=http://api:8000/api/v1
API_VERIFY_SSL=false
